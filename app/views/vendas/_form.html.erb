<%= simple_form_for(@venda) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :valor %>
    <%= f.input :data %>
    <%= f.input :funcionario_id %>
    <%= f.input :cliente_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
